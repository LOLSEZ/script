Reach.Name = "Reach"
Reach.Parent = Main
Reach.BackgroundColor3 = Color3.fromRGB(255, 17, 21)
Reach.Position = UDim2.new(0.055361025, 0, 0.626484573, 0)
Reach.Size = UDim2.new(0, 144, 0, 51)
